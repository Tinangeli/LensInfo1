<Window x:Class="LensInfo1.UserControls.ColorPalette"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LensInfo1.UserControls"
        mc:Ignorable="d"
        Title="ColorPalette" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>

            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0" Background="#FFFFE0"/>
        <StackPanel Grid.Column="1" Background="#FFCC99" />
        <StackPanel Grid.Column="2" Background="#99FF99"/>
        <StackPanel Grid.Column="3" Background="#99CCFF"/>
        <StackPanel Grid.Column="4" Background="#FF9999"/>
        <StackPanel Grid.Column="5" Background="#E6E6FF"/>
        <StackPanel Grid.Column="7" Background="#FFDAB9"/>

    </Grid>
</Window>
